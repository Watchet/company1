<!--logs.wxml-->
<view class="user_banner">
	<view id="user_info">
		<view class="user_img">
			<open-data type="userAvatarUrl" class="user_pho"></open-data>
		</view>
		<view class="user_name">
			<open-data type="userNickName" lang="zh_CN"></open-data>
		</view>
		<view class="member_bank">
			<text>大众会员</text><text> 15165665</text>
		</view>
	</view>
	<view class="cre_wrap">
		<view class="credit flexCenter">
			<view class="cre_img">
				<view class="cre_img1" style="{{img}};background-position:-288px 0px;"></view>
			</view>
			<text>大众会员</text>
		</view>
	</view>
</view>
<view id="user">
	<view class="user_two flexRowBetween">
		<view class="user_credit user_collect">
			<view class="collect_number">{{web_userData.score?web_userData.score:'网络故障'}}</view>
			<view class="collect_txt">我的余额</view>
		</view>
		<view class="user_fill" bindtap="top_in">
			去充值
		</view>
	</view>
	
	<view class="user_all">
		<view class="user_infor flexRowBetween"  bindtap="userInfo">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -81px -230px"></view>
				<text>会员卡</text>
			</view>
			<view class="next_page">
				<view style="width:18rpx;height:32rpx;{{img}};background-position: -262px 0;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="consume">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -61px -230px"></view>
				<text>消费记录</text>
			</view>
			<view class="next_page">
				<view style="width:18rpx;height:32rpx;{{img}};background-position: -262px 0;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="address">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -41px -230px"></view>
				<text>我的相册</text>
			</view>
			<view class="next_page">
				<view style="width:18rpx;height:32rpx;{{img}};background-position: -262px 0;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="mygroup">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -21px -226px;width:34rpx;height:36rpx;"></view>
				<text>我的团购</text>
			</view>
			<view class="next_page">
				<view style="width:18rpx;height:32rpx;{{img}};background-position: -262px 0;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="discount">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: 0px -230px;width:34rpx;"></view>
				<text>我的优惠卷</text>
			</view>
			<view class="next_page">
				<view style="width:18rpx;height:32rpx;{{img}};background-position: -262px 0;"></view>
			</view>
		</view>
	</view>
	<view class="payment" bindtap="payment">
		立即支付
	</view>
</view>
<view class="bottom_spacing"></view>
<view class="navbar">
    <view class="navbar_item" bindtap="intoPathRedirect" data-path="/pages/hair/Index/index">
        <view class="nav_img" style="{{img}};background-position: -54px -160px;"></view>
        <view class="text" >首页</view>
    </view>
    <view class="navbar_item" bindtap="intoPathRedirect" data-path="/pages/hair/Service/service">
        <view class="nav_img"  style="{{img}};background-position: -108px -160px;"></view>
        <view class="text">服务</view>
    </view>
    <view class="navbar_item"  bindtap="intoPathRedirect" data-path="/pages/hair/HairStyle/hairstyle">
        <view class="nav_img"  style="{{img}};background-position: -0px -160px;"></view>
        <view class="text">发型</view>
    </view>
    <view class="navbar_item"  bindtap="intoPathRedirect" data-path="/pages/hair/HairDresser/hairDresser">
        <view class="nav_img"  style="{{img}};background-position: -216px -160px;"></view>
        <view class="text">技师</view>
    </view>
    <view class="navbar_item"  bindtap="intoPathRedirect" data-path="/pages/hair/User/user">
        <view class="nav_img"  style="{{img}};background-position: -189px -160px;"></view>
        <view class="text" style="color:#fb5676">我的</view>
    </view>
</view>