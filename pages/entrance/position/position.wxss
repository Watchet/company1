page{
  width:100%;
  min-height:100%;
  background:#f6f6f6;
}
.clientList_center{
  padding-top:30rpx;
  width:65%;
}
.clientList_right{
  padding-top:30rpx;
  width:35%;
  text-align:right;
}
.position_condition{
  height:76rpx;
  border-bottom:solid 1px #e0e0e0;
}
.position_left,.position_right{
  width:50%;
}
.position_left image{
  width:34rpx;
  height:38rpx;
}
.address image{
  width:34rpx;
  height:38rpx;
  margin-right:20rpx;
}
.position_left image{
  margin-right:20rpx;
}
.position_right {
  text-align:right;
}
.address{
  height:90rpx;
  line-height:90rpx;
  border-bottom:solid 1px #e0e0e0;
}
.client_intro{
  padding:30rpx 2.6%;
  margin-bottom:30rpx
}
.bgs{
  background:#fff;
}
.wxParse-p{
  margin:14rpx 0;
}
.flexs{
  display:flex;
}
.condition{
  flex-wrap:wrap;
}
.condition_item{
  padding:10rpx 18rpx;
  background:#f3feff;
  margin-right:30rpx;
}
.sender{
  width:100%;
  height:60rpx;
}
.sender_img{
  width:50rpx;
  height:50rpx;
  overflow:hidden;
  border-radius:50%;
  margin-right:20rpx;
}
.sender image{
  width:100%;
  height:100%;
}
.position_cont{
  margin-top:20rpx;
}