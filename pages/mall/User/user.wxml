<!--logs.wxml-->
<view class="users">

		<view class="user_info flexRowBetween">
			<view style="width:30%;">
				<view class="user_img">
					<open-data type="userAvatarUrl"></open-data>
				</view>
			</view>
			 <view class="user_word">
			 	<view class="user_name"><open-data type="userNickName" lang="zh_CN"></open-data></view>
			 	<view class="phone">158767678</view>
			 </view>
			 <view style="width:15%;">
		    	<view class="sign_up">
		    		<text>签</text>
		    	</view>
		    </view>
	    	<view class="consult" style="width:15%;" >
				<view style="width:38rpx;height:38rpx;margin:0 auto;{{img}};background-position: 1px -144rpx;"></view>
	    	</view>
		</view>

</view>
<view id="user">
	<view class="user_two flexRowBetween">
		<view class="user_credit user_collect" style="{{fonts[3].font}}" bindtap="intoPath" data-path="/pages/mall/collectData/collectData">
			<view class="collect_number">{{web_collectData?web_collectData:'网络故障'}}</view>
			<view class="collect_txt">我的收藏</view>
		</view>
		<view class="user_credit"  style="{{fonts[3].font}}" bindtap="intoPath" data-path="/pages/mall/userCredit/userCredit">
			<view class="credit_number">{{web_userData.score?web_userData.score:'网络故障'}}</view>
			<view class="credit_txt">我的积分</view>
		</view>
	</view>
	<view class="order" bindtap="intoPath" data-path="/pages/mall/order/order">
		<view class="user_order flexRowBetween">
			<view class="order_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -180px -148rpx;"></view>
				<view class="my_order">我的订单</view>
			</view>
			
			<view class="check_order flexRowBetween">
				<view style="fonts[2].font">
					查看订单
				</view>
				<view class="user_icon1" style="{{img}};background-position: -330px -22px;"></view>
			</view>
		</view>
	</view>
	<view class="process flexRowBetween">
		<view class="peocess_item item_other" bindtap="order">
			<view class="user_icon3" style="{{img}};background-position: -83px -144rpx;"></view>
			<text>待付款</text>
		</view>
		<view class="peocess_item">
			<view class="user_icon3" style="width:46rpx;{{img}};background-position: -61px -144rpx;"></view>
			<text>待发货</text>
		</view>
		<view class="peocess_item">
			<view class="user_icon3" style="width:46rpx;{{img}};background-position: -39px -144rpx;"></view>
			<text>待收货</text>
		</view>
		<view class="peocess_item">
			<view class="user_icon3" style="width:46rpx;{{img}};background-position: -16px -144rpx;"></view>
			<text>待评价</text>
		</view>
		<view class="peocess_item">
			<view class="user_icon3" style="width:46rpx;{{img}};background-position: -110px -144rpx;"></view>
			<text>退换货</text>
		</view>
	</view>
	<view class="user_all" style="margin-bottom:120rpx">
		<view class="user_infor flexRowBetween"  bindtap="intoPath" data-path="/pages/mall/userInfo/userInfo">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -164px -148rpx;"></view>
				<text>我的信息</text>
			</view>
			<view class="next_page">
				<view class="user_icon1" style="{{img}};background-position: -330px -22px;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween"  bindtap="intoPath" data-path="/pages/mall/userGroup/userGroup">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -180px -148rpx;"></view>
				<text>我的团队</text>
			</view>
			<view class="next_page">
				<view class="user_icon1" style="{{img}};background-position: -330px -22px;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="intoPath" data-path="/pages/mall/userDiscount/userDiscount">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -134px -148rpx;"></view>
				<text>我的优惠劵</text>
			</view>
			<view class="next_page">
				<view class="user_icon1" style="{{img}};background-position: -330px -22px;"></view>
			</view>
		</view>
		<view class="user_infor flexRowBetween" bindtap="intoPath" data-path="/pages/mall/manageAddress/manageAddress">
			<view class="user_left flexRowBetween">
				<view class="user_icon" style="{{img}};background-position: -149px -148rpx;"></view>
				<text>我的地址</text>
			</view>
			<view class="next_page">
				<view class="user_icon1" style="{{img}};background-position: -330px -22px;"></view>
			</view>
		</view>
	</view>
</view>

<view class="navbar">
      <view class="navbar_item"  bindtap="intoPathRedi" data-path="/pages/mall/Index/index">
          <view class="nav_img" style="{{img}};width:44rpx;height:42rpx;background-position: -21px -94rpx;">
          </view>
          <view class="text" style="{{fonts[2].font}}">首页</view>
      </view>
    <view class="navbar_item" bindtap="intoPathRedi" data-path="/pages/mall/Sort/sort">
        <view class="nav_img" style="{{img}};width:44rpx;height:42rpx;background-position: -138rpx -94rpx;">
        </view>
        <view class="text" style="{{fonts[2].font}}">分类</view>
    </view>
    <view class="navbar_item" bindtap="intoPathRedi" data-path="/pages/mall/Shopping/shopping">
        <view class="nav_img" style="{{img}};width:42rpx;height:42rpx;background-position: -240rpx -94rpx;">
        </view>
        <view class="text" style="{{fonts[2].font}}">购物车</view>
    </view>
    <view class="navbar_item" bindtap="intoPathRedi" data-path="/pages/mall/User/user">
        <view class="nav_img" style="{{img}};width:42rpx;height:42rpx;background-position: -288rpx -94rpx;">
        </view>
        <view class="text this-text" style="{{fonts[2].font}}">我的</view>
    </view>
</view>